#include <bits/stdc++.h>
using namespace std;
#define ll long long

void solve()
{
    multiset<ll> ms1 = {5, 3, 1, 3, 3, 2, 1, 6};

    auto it = ms1.find(3);                        // find operation // check any element present or not
    if (it != ms1.end())
        cout << "found" << endl;


/*-------------------------------------***------------------------------------------------------------------------------------*/
    it = ms1.lower_bound(3);                     // lower bound operation
    if (it != ms1.end())
    {
        ms1.erase(it);                           // erase specefic index value
        ll val = *it;
        ms1.erase(val);                          // erase all occurance of that value
        ms1.insert(4);
    }
/*----------------------------------------------------------------------------------------------------------------------------*/


    for (auto d : ms1)
        cout << d << " ";
}
int main()
{
    solve();
    cout << endl;
    return 0;
}
