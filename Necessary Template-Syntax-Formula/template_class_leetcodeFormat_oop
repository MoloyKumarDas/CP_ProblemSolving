int main()
{
    int n;
    cin >> n;

    Solution obj;                                 // obj is just an instance of that class.
                                                 // Think of Solution as a blueprint (class)
                                                // and obj as the actual working machine built from that blueprint (object)

    vector<int> result = obj.countBits(n);     // calls the function countBits() from your Solution object obj

    // vector<int> result = countBits(n);
    for (auto d : result)
        cout << d << " ";

    return 0;
}
